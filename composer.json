{
    "name": "alexandret/phpcas-guard-bundle",
    "type": "symfony-bundle",
    "description": "PhpCAS Bundle using guard for Symfony3",
    "homepage": "https://github.com/Alexandre-T/casguard",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Tranchant",
            "email": "alexandre.tranchant@gmail.com"
        }
    ],
    "support": {
        "email": "Alexandre.Tranchant@gmail.com",
        "issues": "https://github.com/Alexandre-T/casguard/issues",
        "source": "https://github.com/Alexandre-T/casguard"
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "jasig/phpcas": "~1.3",
	    "symfony/security-guard": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/routing": "~2.8|~3.0|~4.0",
        "symfony/yaml": "^3.3"
    },
    "autoload": {
        "psr-4": { "AlexandreT\\Bundle\\CasGuardBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "codeception/aspect-mock": "^1.0|^2.0",
        "phpunit/phpunit": "^5.5|^6.3",
        "symfony/debug": "^3.3",
        "symfony/phpunit-bridge": "^3.3"
    }
}
